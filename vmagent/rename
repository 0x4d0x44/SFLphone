//
// RENAME A VOICEMAIL
//
// Exemple of use :
// http://default-6666:735@127.0.0.1/uml/index/rename?f=INBOX&o=msg0000&n=msg1234
//

// Gets folder, current name and new name of the voicemail to rename
$folder   = $_GET['f'];
$old_name = $_GET['o'];
$new_name = $_GET['n'];

echo "  <renamed>";
$agent->rename( $folder , $old_name , $new_name );
echo "</renamed>\n";

?>
