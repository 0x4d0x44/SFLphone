//
// DELETE A VOICEMAIL (TXT + SOUNDS)
//
// Exemple of use :
// http://default-6666:735@127.0.0.1/uml/index/del?f=INBOX&v=msg0000
//

$folder    = $_GET['f'];
$voicemail = $_GET['v'];

echo "  <deleted>";
$agent->delete( $folder , $voicemail );
echo "</deleted>\n";

?>
